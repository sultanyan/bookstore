<%= form_for @publisher do |f| %>
    <div class="form-group">
        <%= f.label :first_name %>
        <%= f.text_field :first_name, class: 'form-control' %>
    </div>

    <div class="form-group">
        <%= f.label :last_name %>
        <%= f.text_field :last_name, class: 'form-control' %>
    </div>

    <%= f.submit 'Update Publisher', class: 'btn btn-primary' %>
    <%= link_to 'Cancel', publisher_path, class: 'btn bnt-default' %>
<% end %>
